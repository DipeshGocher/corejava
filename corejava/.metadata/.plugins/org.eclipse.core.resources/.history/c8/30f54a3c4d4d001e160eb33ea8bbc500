package in.com.iohard;

import java.io.FileWriter;


public class Filewriter1 {
public static void main(String[] args) throws Exception {
	FileWriter writer = new FileWriter ("C:\\Users\\ACER\\OneDrive\\Desktop\\iohard\\dipesh.txt");
	writer.write("helllo i am dipesh");
	writer.write(" finally placed yess!!");
	writer.close();
	System.out.println("data write succesfully");
}
}
